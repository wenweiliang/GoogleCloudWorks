echo -e "Sample\tCoveredSite\tMSIsensorScore" > TCGA.WGS.msisensor.lowpass.txt ; for i in `gsutil ls gs://dinglab/wliang_msi/output/TCGA*/lowpass/TCGA*.gz | awk -F "_dis" '{print $1}'`; do sample=$(echo $i | awk -F "/" '{print $6}'); echo ${sample}; gsutil cat $i | awk -v x=${sample} -F "\t" 'NR==2{OFS="\t"; print x, $1, $3}' >> TCGA.WGS.msisensor.lowpass.txt; done
echo -e "Sample\tCoveredSite\tMSIsensorScore" > TCGA.WGS.msisensor.highpass.txt ; for i in `gsutil ls gs://dinglab/wliang_msi/output/TCGA*/highpass/TCGA*.gz | awk -F "_dis" '{print $1}'`; do sample=$(echo $i | awk -F "/" '{print $6}'); echo ${sample}; gsutil cat $i | awk -v x=${sample} -F "\t" 'NR==2{OFS="\t"; print x, $1, $3}' >> TCGA.WGS.msisensor.highpass.txt; done
